<?xml version="1.0"?>
	<robot name="gemab">

	<material name="blue">
		<color rgba="0 0 .8 1"/>
	</material>

	<material name="black">
		<color rgba="0 0 0 1"/>
	</material>

	<link name="base_footprint"/>

	

	<link name="base_link">
		<visual>
			<geometry>
				<box size="0.47 0.34 0.020"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0.0 0.0 0.06"/>
			<material name="blue"/>
		</visual>
	</link>

	<link name="wheel_left">
		<visual>
			<geometry>
				<cylinder length="0.01" radius="0.09"/>
			</geometry>
			<origin rpy="1.57 0 0" xyz="0 0 0"/>
			<material name="black"/>
		</visual>
	</link>

	<link name="wheel_right">
		<visual>
			<geometry>
				<cylinder length="0.01" radius="0.09"/>
			</geometry>
			<origin rpy="1.57 0 0" xyz="0 0 0"/>
			<material name="black"/>
		</visual>
	</link>

	<joint name="base_joint" type="fixed">
		<parent link="base_footprint"/>
		<child link="base_link"/>
		<origin xyz="0.0 0.0 0.05" rpy="0 0 0"/>
	</joint>

	<joint name="base_to_wheel_left" type="continuous">
		<parent link="base_link"/>
		<child link="wheel_left"/>
		<origin xyz="0.235 -0.185 0.045"/>
		<axis xyz="0 0 1"/>
    </joint>

    <joint name="base_to_wheel_right" type="continuous">
		<parent link="base_link"/>
		<child link="wheel_right"/>
		<origin xyz="0.235 0.185 0.045"/>
		<axis xyz="0 0 1"/>
    </joint>

	</robot>
